{
    "name": "nowise/uup-build-system",
    "version": "1.5.1",
    "description": "Target build system similar to make with complex dependency tree",
    "type": "library",
    "keywords": [
        "build system",
        "target",
        "dependencies",
        "uup"
    ],
    "homepage": "https://nowise.se/oss/uup/build-system",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Anders Lövgren",
            "email": "andlov@nowise.se"
        },
        {
            "name": "Anders Lövgren",
            "email": "anders.lovgren@interactivesolutions.se"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "nowise/uup-application": "^1.0",
        "nowise/uup-application-options": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "UUP\\": "source/",
            "UUP\\Example\\": "example/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UUP\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/pbsmake"
    ]
}
