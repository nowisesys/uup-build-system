{
    "name": "nowise/uup-build-system",
    "description": "Target build system similar to make with complex dependency tree",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Anders Lövgren",
            "email": "andlov@nowise.se"
        },
        {
            "name": "Anders Lövgren",
            "email": "anders.lovgren@interactivesolutions.se"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../uup-application"
        },
        {
            "type": "path",
            "url": "../uup-application-options"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "nowise/uup-application": "@dev",
        "nowise/uup-application-options": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "UUP\\": "source/",
            "UUP\\Example\\": "example/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UUP\\Tests\\": "tests/"
        }
    }
}
